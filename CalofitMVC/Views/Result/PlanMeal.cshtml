<script>
    document.addEventListener("DOMContentLoaded", function () {
        const heading1 = document.getElementById('heading1');
        const heading2 = document.getElementById('heading2');
        const heading3 = document.getElementById('heading3');
        const heading4 = document.getElementById('heading4');
        const heading5 = document.getElementById('heading5');
        const heading6 = document.getElementById('heading6');
        const heading7 = document.getElementById('heading7');
        const heading8 = document.getElementById('heading8');

        const collapse1 = document.getElementById('collapse1');
        const collapse2 = document.getElementById('collapse2');
        const collapse3 = document.getElementById('collapse3');
        const collapse4 = document.getElementById('collapse4');
        const collapse5 = document.getElementById('collapse5');
        const collapse6 = document.getElementById('collapse6');
        const collapse7 = document.getElementById('collapse7');
        const collapse8 = document.getElementById('collapse8');

        const headings = [
            heading1, heading2, heading3, heading4, heading5, heading6, heading7, heading8
        ];

        const buttons = document.querySelectorAll('.btn-link');

        let activeButton = null;

        buttons.forEach((button, index) => {
            button.addEventListener("click", function () {
                if (activeButton !== this) {
                    if (activeButton) {
                        activeButton.classList.remove('active');
                    }
                    this.classList.add('active');
                    activeButton = this;
                }
            });
        });

        let activeHeading = null;

        headings.forEach((heading, index) => {
            heading.addEventListener("click", function () {
                if (activeHeading !== this) {
                    if (activeHeading) {
                        activeHeading.classList.remove('active');
                        document.getElementById(`collapse${headings.indexOf(activeHeading) + 1}`).style.display = 'none';
                    }
                    this.classList.add('active');
                    activeHeading = this;
                    document.getElementById(`collapse${index + 1}`).style.display = 'block';
                }
            });
        });

        function hideAllCollapse() {
            collapse1.style.display = 'none';
            collapse2.style.display = 'none';
            collapse3.style.display = 'none';
            collapse4.style.display = 'none';
            collapse5.style.display = 'none';
            collapse6.style.display = 'none';
            collapse7.style.display = 'none';
            collapse8.style.display = 'none';
        }

        resFoods.forEach(item => {
            item.addEventListener("click", function (event) {
                event.preventDefault();
                if (!this.classList.contains("active")) {
                    removeActiveClass(resRecipes);
                    this.classList.add('active');
                    foodsArea.style.display = 'inline-table';
                    recipesArea.style.display = 'none';
                }
            });
        });

        resRecipes.forEach(item => {
            item.addEventListener("click", function (event) {
                event.preventDefault();
                if (!this.classList.contains("active")) {
                    removeActiveClass(resFoods);
                    this.classList.add('active');
                    recipesArea.style.display = 'inline-table';
                    foodsArea.style.display = 'none';
                }
            });
        });

        function removeActiveClass(elements) {
            elements.forEach(item => {
                item.classList.remove('active');
            });
        }
    });
</script>